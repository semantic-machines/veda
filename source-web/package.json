  },
  "repository": {
    "type": "git",
    "url": "git://github.com/semantic-machines/veda.git"
  },
  "license": "AGPL-3.0-or-later",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-instanceof": "^7.12.1",
    "@babel/plugin-transform-modules-systemjs": "^7.12.1",
    "@babel/preset-env": "^7.14.4",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.15.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-security-node": "^1.1.0",
    "rollup": "^2.33.1",
    "rollup-plugin-terser": "^7.0.2"
  },
  "dependencies": {
    "ace-builds": "^1.4.12",
    "anymatch": "^3.1.2",
    "autosize": "^4.0.2",
    "body-parser": "^1.19.0",
    "bootstrap": "^3.4.1",
    "bootstrap-contextmenu": "^1.0.0",
    "bowser": "^2.11.0",
    "chokidar": "^3.5.2",
    "construct-style-sheets-polyfill": "^2.4.16",
    "core-js": "^3.14.0",
    "cors": "^2.8.5",
    "cropperjs": "^1.5.9",
    "eonasdan-bootstrap-datetimepicker": "^4.17.47",
    "express": "^4.17.2",
    "file-saver": "^1.3.8",
    "font-awesome": "^4.7.0",
    "fullcalendar": "^3.0.1",
    "jquery": "^3.3.1",
    "jquery-touchswipe": "^1.6.19",
    "jquery-ui-dist": "^1.12.1",
    "jsplumb": "^1.7.9",
    "jszip": "^3.1.3",
    "marked": ">=4.0.10",
    "moment": "^2.29.1",
    "mustache": "^4.0.1",
    "promis": "^1.1.4",
    "regenerator-runtime": "^0.13.7",
    "rollup-plugin-import-map": "^2.2.2",
    "symbol-es6": "^0.1.2",
    "systemjs": "^6.7.1",
    "systemjs-plugin-css": "^0.1.37",
    "vis-network": "^8.5.2"
  },
  "scripts": {
    "build": "npm run clean && npm run build-browser-scripts && npm run build-templates && npm run build-server-scripts && npm run update-version",
    "clean": "find ../public/* ! -name \"ontology.json*\" -delete 2>/dev/null || true",
    "build-browser-scripts": "babel --config-file ./.babelrc . --out-dir ../public --source-maps --copy-files",
    "build-templates": "babel --config-file ./.babelrc ../ontology/**/**/**/**/templates/ --out-dir ../public/templates --source-maps",
    "build-server-scripts": "rm -Rf ../public/js/server && rollup -c && cat js/server/lib/*.js ../public/js/server/*.js > .tmp-build && rm -f ../public/js/server/*.js && mv .tmp-build ../public/js/server/server.bundle.js",
    "update-version": "chmod +x ./update-version.sh && ./update-version.sh",
    "watch": "npm run watch-browser & npm run watch-common & npm run watch-templates",
    "watch-browser": "babel --config-file ./.babelrc ./js/browser --out-dir ../public/js/browser --source-maps --watch",
    "watch-common": "babel --config-file ./.babelrc ./js/common --out-dir ../public/js/common --source-maps --watch",
    "watch-templates": "babel --config-file ./.babelrc ../ontology/**/**/**/**/templates/ --out-dir ../public/templates --source-maps --watch"
  }
}
