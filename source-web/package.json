  "scripts": {
    "test": "npm run test-backend && npm run test-frontend",
    "test-backend": "node ./tests/backend/index.js",
    "test-frontend": "testcafe -d --concurrency 1 --skip-js-errors --speed 0.7 \"firefox:/usr/bin/firefox:headless\" ./tests/testcafe --screenshots-on-fails --screenshots-path ./screenshots --selector-timeout 600000 --assertion-timeout 600000",
    "build": "npm run clean && npm run build-browser-scripts && npm run build-templates && npm run build-server-scripts && npm run update-version",
    "clean": "find ../public/* ! -name \"ontology.json*\" -delete 2>/dev/null || true",
    "build-browser-scripts": "babel --config-file ./.babelrc . --out-dir ../public --copy-files",
    "build-templates": "babel --config-file ./.babelrc ../ontology/**/**/**/**/templates/ --out-dir ../public/templates",
    "build-server-scripts": "rm -Rf ../public/js/server && rollup -c && cat js/server/lib/*.js ../public/js/server/*.js > .tmp-build && rm -f ../public/js/server/*.js && mv .tmp-build ../public/js/server/server.bundle.js",
    "update-version": "chmod +x ./update-version.sh && ./update-version.sh",
    "watch": "npm run watch-browser & npm run watch-common & npm run watch-templates & npm run watch-portal",
    "watch-browser": "babel --config-file ./.babelrc ./js/browser --out-dir ../public/js/browser --watch",
    "watch-common": "babel --config-file ./.babelrc ./js/common --out-dir ../public/js/common --watch",
    "watch-templates": "babel --config-file ./.babelrc ../ontology/**/**/**/**/templates/ --out-dir ../public/templates --watch",
    "watch-portal": "babel --config-file ./.babelrc ./portal/**/**/**/**/templates/ --out-dir ../public/templates --watch"
  }
}
