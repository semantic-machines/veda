  v-ui:forClass v-s:InternalAccountingDocument ;
  v-ui:forProperty v-s:attachment ;
  v-ui:minCardinality 0 ;
  v-ui:maxCardinality 1 ;
  rdfs:label "Attachments specification for InternalAccountingDocument class"@en ;
