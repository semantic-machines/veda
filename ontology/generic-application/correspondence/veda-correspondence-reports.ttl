<http://semantic-machines.com/veda/veda-correspondence-reports>
  rdf:type owl:Ontology ;
  rdfs:label "Онтология системы Veda. Корреспонденция. Отчеты."@ru ;
  rdfs:label "Veda system ontology. Correspondence. Reports."@en ;
#  owl:versionInfo "1.3" ;
  v-s:loadPriority 6 ;
.

# REPORTS --------------------------------------------------------------

v-s:blankCreateCountOfUsersTasks_report
  a v-fc:Blank;
  rdfs:label "Количество задач"@ru ;
  rdfs:label "Number of tasks"@en ;
  v-fc:targetType v-s:CountOfUsersTasks_report_Parameters ;
.

v-s:blankCreateCompletedTasksCount_Report
  a v-fc:Blank;
  rdfs:label "Количество выполненных задач"@ru ;
  rdfs:label "Number of competed tasks"@en ;
  v-fc:targetType v-s:CompletedTasksCount_Report_Parameters ;
.

v-s:CountOfUsersTasks_Report
  rdf:type v-s:Report ;
  v-s:filePath "/reports/Documents/CountOfUsersTasks_Report" ;
  v-s:fileFormat "pdf";
  rdfs:label "Количество задач"@ru ;
  rdfs:label "Number of tasks"@en ;
.

v-s:CompletedTasksCount_Report
  rdf:type v-s:Report ;
  v-s:filePath "/reports/Documents/CompletedTasksCount_Report" ;
  v-s:fileFormat "pdf";
  rdfs:label "Количество выполненных задач"@ru ;
  rdfs:label "Number of competed tasks"@en ;
.

v-s:CountOfUsersTasks_report_Parameters
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:SystemThing ;
  rdfs:label "Параметры отчета по количеству задач "@ru ;
  rdfs:label "Parameters for number of tasks report"@en ;
  v-ui:hasTemplate v-s:CountOfUsersTasks_report_ParametersTemplate ;
.

v-s:CompletedTasksCount_Report_Parameters
  rdf:type owl:Class ;
  rdfs:subClassOf v-s:SystemThing ;
  rdfs:label "Параметры отчета по количеству выполненных задач "@ru ;
  rdfs:label "Parameters for number of completed tasks report"@en ;
  v-ui:hasTemplate v-s:CompletedTasksCount_Report_ParametersTemplate ;
.

# TEMPLATES ------------------------------------------------------------

v-s:CountOfUsersTasks_report_ParametersTemplate
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Шаблон Параметров отчета по количеству задач"@ru ;
  rdfs:label "NumberOfTasks_ReportParameters template"@en ;
  v-ui:forClass v-s:CountOfUsersTasks_report_Parameters ;
  v-ui:template
  """
<div class="container sheet">
  <h3 about="v-s:CountOfUsersTasks_report_Parameters" property="rdfs:label"></h3>
  <div class="row">
    <div class="col-md-3">
      <em about="v-s:dateFrom" property="rdfs:label"></em>
      <div property="v-s:dateFrom" class="view -edit -search"></div>
      <veda-control property="v-s:dateFrom" data-type="dateTime" class="-view edit search"></veda-control>
    </div>
    <div class="col-md-3">
      <em about="v-s:dateTo" property="rdfs:label"></em>
      <div property="v-s:dateTo" class="view -edit -search"></div>
      <veda-control property="v-s:dateTo" data-type="dateTime" class="-view edit search"></veda-control>
    </div>
  </div>
  <div class="row">
    <div class="col-md-6">
      <em about="v-s:checkedDepartment" property="rdfs:label"></em>
      <div rel="v-s:checkedDepartment" data-template="v-ui:LabelTemplate" class="view edit -search"></div>
      <veda-control rel="v-s:checkedDepartment" data-type="link" class="-view edit search fulltext"></veda-control>
    </div>
  </div>
  <br><br>
  <div class="actions">
    <button type="button" class="action btn btn-success -view edit -search" id="createReport" about="v-s:CreateReport" property="rdfs:label"/>
  </div>
</div>
<script>
  $('#createReport', template).on('click', function () {
    veda.Util.createReport(individual, 'v-s:CountOfUsersTasks_Report');
  });
  //# sourceURL=v-s:CountOfUsersTasks_report_ParametersTemplate
</script>
""" ;
.

