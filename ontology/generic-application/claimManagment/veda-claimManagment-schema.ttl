  v-ui:maxCardinality 1 ;
  v-ui:minCardinality 0 ;
  v-ui:queryPrefix "('@' == 'v-s:StatusAcceptedPartially' || '@' == 'v-s:StatusRejected' || '@' == 'v-s:StatusAccepted')"^^xsd:string ;
