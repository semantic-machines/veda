v-s:hasDecreeKind
  rdf:type owl:ObjectProperty ;
  rdfs:label "Вид документа"@ru ;
  rdfs:label "Decree kind"@en ;
  rdfs:range v-s:DecreeKind ;
  rdfs:domain v-s:Decree ;
.
