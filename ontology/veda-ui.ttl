v-ui:RabbitHole
  rdf:type v-ui:ClassTemplate ;
  rdfs:label "Дерево докумментов"@ru ;
  rdfs:label "Tree dokummentov"@en ;
  v-ui:template """
<script>
  var text = "<span>" + mkLabel(individual) + "</span>";
  if (individual.hasValue("v-s:backwardTarget")) {
    travel(individual["v-s:backwardTarget"][0]);
  } else if (individual.hasValue("v-s:parent")) {
    travel(individual["v-s:parent"][0]);
  }
  $("small", template).append(text);
  function travel(individual) {
    text = "<a href='#/" + individual.id + "'>" + mkLabel(individual) + "</a>" + " &rarr; " + text;
    if (!individual.hasValue("v-s:parent") && !individual.hasValue("v-s:backwardTarget")) { return; }

    if (individual.hasValue("v-s:backwardTarget")) {
    travel(individual["v-s:backwardTarget"][0]);
  } else if (individual.hasValue("v-s:parent")) {
    travel(individual["v-s:parent"][0]);
  }

  }
  function mkLabel(individual) {
    return individual["rdf:type"][0]["rdfs:label"].join(" ")// + ": " + ( individual.hasValue("rdfs:label") ? individual["rdfs:label"].join(" ") : individual.id )
  }
</script>
<h5><small></small></h5>
  """ ;
.

