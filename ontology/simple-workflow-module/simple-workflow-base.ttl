    routes['instruction'][simple+'comment'] = [new String('Отсканировать и направить на ознакомление')];

  }

  //--------------------------------------------------------------------------------Акт уничтожения СИЗ-------
  if (doc['rdf:type'][0].id === "mnd-s:ActOfPpeDestruction") {
    routes['coordination1'][simple+'participant'] = doc["v-s:member"];
